{
  "name": "cooltronicpl/craft-files-autoversioning",
  "description": "A simple yet powerful Twig extension for CraftCMS. It assists in cache-busting your assets, ensuring that users always receive the latest versions of your files.",
  "version": "2.2.0",
  "type": "craft-plugin",
  "keywords": [
    "static-files-autoversioning",
    "craft-cms-cache-busting-plugin",
    "twig-extension-for-asset-versioning",
    "craft-cms-static-asset-optimization",
    "cache-busting-and-versioning-for-craft-cms"
  ],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "CoolTRONIC.pl sp. z o.o. (LLC)",
      "homepage": "https://cooltronic.pl",
      "role": "Maintainer"
    },
    {
      "name": "Pawe≈Ç Potacki",
      "homepage": "https://potacki.com",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/cooltronicpl/craft-files-autoversioning",
    "docs": "https://github.com/cooltronicpl/craft-files-autoversioning/blob/master/README.md",
    "issues": "https://github.com/cooltronicpl/craft-files-autoversioning/issues"
  },
  "require": {
    "craftcms/cms": "^5.0.0.alpha|^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "cooltronicpl\\autoversioning\\": "src/"
    }
  },
  "extra": {
    "handle": "craft3-files-autoversioning",
    "class": "cooltronicpl\\autoversioning\\Autoversioning",
    "name": "Static Files Autoversioning",
    "description": "Twig extension which enable to automatically version your static files.",
    "hasCpSettings": false,
    "hasCpSection": false
  }
}
